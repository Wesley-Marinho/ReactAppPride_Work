{
  "expo": {
    "name": "Pride&Work",
    "slug": "HackaPride",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/pride_icon.png",
    "splash": {
      "image": "./assets/Pride.png",
      "resizeMode": "contain",
      "backgroundColor": "#E2E1EB"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/pride_icon.png",
        "backgroundColor": "#E2E1EB"
      },
      "package": "com.Pride.Pride_Work",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
